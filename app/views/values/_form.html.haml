= form_for @value do |f|
  -if @value.errors.any?
    #error_explanation
      %h2= "#{pluralize(@value.errors.count, "error")} prohibited this value from being saved:"
      %ul
        - @value.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :name
    = f.text_field :name
  .field
    = f.label :descriptor_id
    = collection_select :value, :descriptor_id, Descriptor.all, :id, :name
  .actions
    = f.submit 'Save'

